# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid "Even scraps can be salvaged. We’re not picky out here."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid "Got extras? Let's make a deal."
msgstr ""

