# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"I've heard tales of a giant shark that drags entire villages underwater. Be "
"careful out there."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid "The Prince means well, but kindness doesn’t patch leaks."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"Before he left, the King provided for all of us. Now we scavenge to survive."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"We used to count stars for fun. Now we count them to forget we’re hungry."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"The Prince says we’re safe. He hasn’t seen the shark fins circling our "
"floating villages!"
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"The Prince tries, but he’s no king. Maybe one day he can find his lost "
"father."
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid "I swear I saw an eye the size of a bucket beneath the waves!"
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid ""
"I might be going crazy, but I swear I saw something looking at me from the "
"sky! It had red glowing eyes and just vanished!"
msgstr ""

# UnityGame/Assets/Localization/BaseTranslation.cs:46 
# UnityGame/Assets/Localization/BaseTranslation.cs:348 
# UnityGame/Assets/Localization/T.cs:84 
# UnityGame/Assets/Blocksters/Scripts/AI/SpeakBubbleControl.cs:94 
msgid "The King used to walk among us. Not above us. That’s why we miss him."
msgstr ""
